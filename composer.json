{
    "name": "jjpmann/ee-addons",
    "type": "ee3-addon",
    "description": "Base addons builder for ExpressionEngine 2",
    "keywords": ["eecms", "addons", "modules", "extensions", "fieldtypes"],
    "homepage": "http://github.com/jjpmann/ee-addons",
    "license": "MIT",
    "authors" : [
        {
            "name": "Jerry Price",
            "email": "jjpmann@gmail.com",
            "homepage": "http://github.com/jjpmann"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:jjpmann/installers.git"
        }
